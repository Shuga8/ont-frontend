{
  "rewrites": [
    {
      "source": "/admin/login",
      "destination": "/admin/login"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    },
    {
      "source": "/survey",
      "destination": "/survey"
    },
    {
      "source": "/401",
      "destination": "/401"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "trailingSlash": false
}
